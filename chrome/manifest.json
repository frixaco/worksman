{
  "manifest_version": 3,
  "name": "Worksman",
  "version": "1.0.0",
  "description": "Manage tabs and workspaces",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "popup.html"
  },

  "permissions": ["scripting", "alarms", "tabs", "storage", "activeTab"],
  "host_permissions": ["<all_urls>", "https://api.tabsync.frixaco.com/"]
}
